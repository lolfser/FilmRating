{
    "name": "lolfser/filmrating",
    "type": "project",
    "description": "Film-Rating",
    "keywords": ["film", "movie", "rating"],
    "repositories": [
        {
          "type": "vcs",
          "url": "git@github.com:lolfser/FilmRating-Models.git"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^v2.0",
        "laravel/framework": "^v12.25",
        "laravel/jetstream": "^v5.1",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "lolfser/filmrating-models": "dev-main",
        "pkeidel/dbtolaravel": "^2.0.1",
        "tightenco/ziggy": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/larastan": "^3.6",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.3",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Models\\": "vendor/lolfser/filmrating-models/App/Models/",
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpstan": [
            "./vendor/bin/phpstan analyse --configuration dev/phpstan/phpstan.neon"
        ],
        "phpstan:baseline": [
            "./vendor/bin/phpstan analyse --configuration dev/phpstan/phpstan.neon --generate-baseline dev/phpstan/phpstan-baseline.neon"
        ],
        "sec-check": [
            "composer update --dry-run roave/security-advisories && npm audit"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
